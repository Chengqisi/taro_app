.homeCard {
  width: 700rpx;
  margin: 20rpx auto;
  min-height: 300rpx;
  border-radius: 16rpx;
  background-color: #fff;
  padding-bottom: 20rpx;
}
.homeCard .nav {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  padding: 30rpx 0 0 30rpx;
}
.homeCard .nav .img {
  display: block;
  width: 60rpx;
  height: 60rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.homeCard .title {
  font-size: 50rpx;
  font-weight: 600;
  color: #333;
  width: -webkit-max-content;
  width: -moz-max-content;
  width: max-content;
  margin: auto;
}
.homeCard .box {
  margin: auto;
  margin-top: 20rpx;
  font-size: 26rpx;
  color: #666;
  max-width: 600rpx;
  text-align: center;
}

